<% layout('layouts/boilerplate') %>
    <div class="row">
        <h1 class="text-center">NAY</h1>
        <div class="col-6 offset-3">
            <form action="/reviews" method="POST" novalidate class="validated-form">
                
                <div class="text-center">
                    <img alt="" src="data:image/png;base64,">
                </div>
                <div class="mb-3">
                    <label class="form-label invisible" for="nay">Nay</label>
                    <input id="nay" class="form-check-input invisible" type="checkbox" value="true" name="review[nay]" checked="true">
                </div>

                <div class="mb-3 text-center">
                    <button class="btn btn-success btn-lg">Submit</button>
                </div>

            </form>
        </div>
    </div>